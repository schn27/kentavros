<?xml version="1.0"?>
<propeller name="kentavros-rearprop">
	<ixx unit="KG*M2">  0.001 </ixx>
	<diameter unit="M"> 0.5 </diameter>
	<numblades> 2 </numblades>
	<gearratio> 1 </gearratio>
	<minpitch> 10.0 </minpitch>
	<maxpitch> 10.0 </maxpitch>
	<constspeed> 0 </constspeed>
	<ct_factor> 0.24 </ct_factor>
	<cp_factor> 0.14 </cp_factor>
	
	<!-- thrust coefficient as a function of advance ratio and blade angle -->
	<table name="C_THRUST" type="internal">
		<tableData>
		0.0	1.0
		1.0	1.0
		</tableData>
	</table>

	<!-- power coefficient as a function of advance ratio and blade angle -->
	<table name="C_POWER" type="internal">
		<tableData>
		0.0	1.0
		1.0	1.0
		</tableData>
	</table>
</propeller>
